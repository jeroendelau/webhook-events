{
    "name": "stareditions/webhook-event-server",
    "require": {
        "php": "^8.0",
        "spatie/laravel-webhook-server": "^3.1",
        "illuminate/database": "^8.70",
        "illuminate/support": "^8.70",
        "illuminate/routing": "^8.70",
        "illuminate/http": "^8.70",
        "illuminate/auth": "^8.70"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "StarEditions\\WebhookEvent\\": "src",
            "StarEditions\\WebhookEvent\\Database\\Factories\\": "src/database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StarEditions\\WebhookEvent\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "StarEditions\\WebhookEvent\\WebhookEventServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
